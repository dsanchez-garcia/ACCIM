Output:Variable, People Block1:Zone1, Zone Thermal Comfort Clothing Value, Monthly;
Output:Variable, People Block1:Zone1, Zone Thermal Comfort Clothing Value, RunPeriod;

EnergyManagementSystem:Actuator,
   clo_value,
   custom_clo_sch_mod,
   Schedule:Compact,
   Schedule Value;

EnergyManagementSystem:GlobalVariable,
clo_adjusted;

EnergyManagementSystem:TrendVariable,
clo_trend_var,
clo_adjusted,
1;

EnergyManagementSystem:ProgramCallingManager,
   adjust_clo,
   BeginTimestepBeforePredictor,
   adjust_clo;

EnergyManagementSystem:Program,
   adjust_clo,
set pmv_prev_val = @TrendValue pmv_trend_var 1,
set clo_prev_val = @TrendValue clo_trend_var 1,
if pmv_prev_val > 0.5,
set clo_adjusted = clo_prev_val-0.1,
elseif pmv_prev_val < -0.5,
set clo_adjusted = clo_prev_val+0.1,
endif;

EnergyManagementSystem:ProgramCallingManager,
   set_clo,
   BeginTimestepBeforePredictor,
   set_clo;

EnergyManagementSystem:Program,
   set_clo,
set clo_value = clo_adjusted;

EnergyManagementSystem:OutputVariable,
clo_adjusted,
clo_adjusted,
Averaged,
ZoneTimestep,
,
clo;

