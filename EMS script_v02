! 1.3. Sensors, actuators and global variable objects for all zones

ENERGYMANAGEMENTSYSTEM:GLOBALVARIABLE,
    CoolingSeason;            !- Erl Variable 1 Name

ENERGYMANAGEMENTSYSTEM:GLOBALVARIABLE,
    CoolSeasonEnd;            !- Erl Variable 1 Name

ENERGYMANAGEMENTSYSTEM:GLOBALVARIABLE,
    CoolSeasonStart;          !- Erl Variable 1 Name

! 2. Stage: Programs

! 2.1. ProgramCallingManager objects

! 2.1.1. ProgramCallingManager objects for all zones

ENERGYMANAGEMENTSYSTEM:PROGRAMCALLINGMANAGER,
    set_cooling_season_input_data,    !- Name
    BeginTimestepBeforePredictor,    !- EnergyPlus Model Calling Point
    set_cooling_season_input_data;    !- Program Name 1

ENERGYMANAGEMENTSYSTEM:PROGRAMCALLINGMANAGER,
    set_cooling_season,       !- Name
    BeginTimestepBeforePredictor,    !- EnergyPlus Model Calling Point
    set_cooling_season;       !- Program Name 1

 ! 2.1.2. ProgramCallingManager objects for Block1:Zone1

 ENERGYMANAGEMENTSYSTEM:PROGRAMCALLINGMANAGER,
    set_zone_input_data_Block1_Zone1,    !- Name
    BeginTimestepBeforePredictor,    !- EnergyPlus Model Calling Point
    set_zone_input_data_Block1_Zone1;    !- Program Name 1

ENERGYMANAGEMENTSYSTEM:PROGRAMCALLINGMANAGER,
    apply_aPMV_Block1_Zone1,    !- Name
    BeginTimestepBeforePredictor,    !- EnergyPlus Model Calling Point
    apply_aPMV_Block1_Zone1;    !- Program Name 1

ENERGYMANAGEMENTSYSTEM:PROGRAMCALLINGMANAGER,
    monitor_aPMV_Block1_Zone1,    !- Name
    BeginTimestepBeforePredictor,    !- EnergyPlus Model Calling Point
    monitor_aPMV_Block1_Zone1;    !- Program Name 1

ENERGYMANAGEMENTSYSTEM:PROGRAMCALLINGMANAGER,
    count_aPMV_comfort_hours_Block1_Zone1,    !- Name
    BeginTimestepBeforePredictor,    !- EnergyPlus Model Calling Point
    count_aPMV_comfort_hours_Block1_Zone1;    !- Program Name 1

 ! 2.1.3. ProgramCallingManager objects for Block1:Zone2

ENERGYMANAGEMENTSYSTEM:PROGRAMCALLINGMANAGER,
    set_zone_input_data_Block1_Zone2,    !- Name
    BeginTimestepBeforePredictor,    !- EnergyPlus Model Calling Point
    set_zone_input_data_Block1_Zone2;    !- Program Name 1

ENERGYMANAGEMENTSYSTEM:PROGRAMCALLINGMANAGER,
    apply_aPMV_Block1_Zone2,    !- Name
    BeginTimestepBeforePredictor,    !- EnergyPlus Model Calling Point
    apply_aPMV_Block1_Zone2;    !- Program Name 1

ENERGYMANAGEMENTSYSTEM:PROGRAMCALLINGMANAGER,
    monitor_aPMV_Block1_Zone2,    !- Name
    BeginTimestepBeforePredictor,    !- EnergyPlus Model Calling Point
    monitor_aPMV_Block1_Zone2;    !- Program Name 1

ENERGYMANAGEMENTSYSTEM:PROGRAMCALLINGMANAGER,
    count_aPMV_comfort_hours_Block1_Zone2,    !- Name
    BeginTimestepBeforePredictor,    !- EnergyPlus Model Calling Point
    count_aPMV_comfort_hours_Block1_Zone2;    !- Program Name 1

! 2.2. Program objects

! 2.2.1. Program objects for all zones

ENERGYMANAGEMENTSYSTEM:PROGRAM,
    set_cooling_season_input_data,    !- Name
    set CoolSeasonStart = 91,
    set CoolSeasonEnd = 274;

ENERGYMANAGEMENTSYSTEM:PROGRAM,
    set_cooling_season,       !- Name
    if CoolSeasonEnd > CoolSeasonStart,
        if (DayOfYear >= CoolSeasonStart) && (DayOfYear < CoolSeasonEnd),
            set CoolingSeason = 1,
        else,                 
            set CoolingSeason = 0,
        endif,                
    elseif CoolSeasonStart > CoolSeasonEnd,
        if (DayOfYear >= CoolSeasonStart) || (DayOfYear < CoolSeasonEnd),
            set CoolingSeason = 1,
        else,                 
            set CoolingSeason = 0,
        endif,                
    endif;                

! 2.2.2. Program objects for Block1:Zone1

ENERGYMANAGEMENTSYSTEM:PROGRAM,
    set_zone_input_data_Block1_Zone1,    !- Name
    set adap_coeff_cooling_Block1_Zone1 = 0.3,
    set adap_coeff_heating_Block1_Zone1 = -0.3,
    set pmv_cooling_sp_Block1_Zone1 = 0.5,
    set pmv_heating_sp_Block1_Zone1 = -0.5,
    set tolerance_cooling_sp_cooling_season_Block1_Zone1 = -0.1,
    set tolerance_cooling_sp_heating_season_Block1_Zone1 = -0.1,
    set tolerance_heating_sp_cooling_season_Block1_Zone1 = 0.25,
    set tolerance_heating_sp_heating_season_Block1_Zone1 = 0.25;

ENERGYMANAGEMENTSYSTEM:PROGRAM,
    apply_aPMV_Block1_Zone1,    !- Name
    if CoolingSeason == 1,
        set adap_coeff_Block1_Zone1 = adap_coeff_cooling_Block1_Zone1,
        set tolerance_cooling_sp_Block1_Zone1 = tolerance_cooling_sp_cooling_season_Block1_Zone1,
        set tolerance_heating_sp_Block1_Zone1 = tolerance_heating_sp_cooling_season_Block1_Zone1,
    elseif CoolingSeason == 0,
        set adap_coeff_Block1_Zone1 = adap_coeff_heating_Block1_Zone1,
        set tolerance_cooling_sp_Block1_Zone1 = tolerance_cooling_sp_heating_season_Block1_Zone1,
        set tolerance_heating_sp_Block1_Zone1 = tolerance_heating_sp_heating_season_Block1_Zone1,
    endif,
    set aPMV_H_SP_noTol_Block1_Zone1 = pmv_heating_sp_Block1_Zone1/(1+adap_coeff_Block1_Zone1*pmv_heating_sp_Block1_Zone1),
    set aPMV_C_SP_noTol_Block1_Zone1 = pmv_cooling_sp_Block1_Zone1/(1+adap_coeff_Block1_Zone1*pmv_cooling_sp_Block1_Zone1),
    set aPMV_H_SP_Block1_Zone1 = aPMV_H_SP_noTol_Block1_Zone1+tolerance_heating_sp_Block1_Zone1,
    set aPMV_C_SP_Block1_Zone1 = aPMV_C_SP_noTol_Block1_Zone1+tolerance_cooling_sp_Block1_Zone1,
    if People_Occupant_Count_Block1_Zone1 > 0,
        if aPMV_H_SP_Block1_Zone1 < 0,
            set PMV_H_SP_act_Block1_Zone1 = aPMV_H_SP_Block1_Zone1,
        else,
            set PMV_H_SP_act_Block1_Zone1 = 0,
        endif,
        if aPMV_C_SP_Block1_Zone1 > 0,
            set PMV_C_SP_act_Block1_Zone1 = aPMV_C_SP_Block1_Zone1,
        else,
            set PMV_C_SP_act_Block1_Zone1 = 0,
        endif,
    else,
        set PMV_H_SP_act_Block1_Zone1 = -100,
        set PMV_C_SP_act_Block1_Zone1 = 100,
    endif;

ENERGYMANAGEMENTSYSTEM:PROGRAM,
    monitor_aPMV_Block1_Zone1,    !- Name
    set aPMV_Block1_Zone1 = PMV_Block1_Zone1/(1+adap_coeff_Block1_Zone1*PMV_Block1_Zone1);

ENERGYMANAGEMENTSYSTEM:PROGRAM,
    count_aPMV_comfort_hours_Block1_Zone1,    !- Name
    if aPMV_Block1_Zone1 < aPMV_H_SP_noTol_Block1_Zone1,
        set comfhour_Block1_Zone1 = 0,
        set discomfhour_cold_Block1_Zone1 = 1*ZoneTimeStep,
        set discomfhour_heat_Block1_Zone1 = 0,
    elseif aPMV_Block1_Zone1 > aPMV_C_SP_noTol_Block1_Zone1,
        set comfhour_Block1_Zone1 = 0,
        set discomfhour_cold_Block1_Zone1 = 0,
        set discomfhour_heat_Block1_Zone1 = 1*ZoneTimeStep,
    else, 
        set comfhour_Block1_Zone1 = 1*ZoneTimeStep,
        set discomfhour_cold_Block1_Zone1 = 0,
        set discomfhour_heat_Block1_Zone1 = 0,
    endif,                
    if People_Occupant_Count_Block1_Zone1 > 0,
        set occupied_hour_Block1_Zone1 = 1*ZoneTimeStep,
    else,                 
        set occupied_hour_Block1_Zone1 = 0,
    endif,                
    set discomfhour_Block1_Zone1 = discomfhour_cold_Block1_Zone1 + discomfhour_heat_Block1_Zone1;

! 2.2.3. Program objects for Block1:Zone2

ENERGYMANAGEMENTSYSTEM:PROGRAM,
    set_zone_input_data_Block1_Zone2,    !- Name
    set adap_coeff_cooling_Block1_Zone2 = 0.25,
    set adap_coeff_heating_Block1_Zone2 = -0.25,
    set pmv_cooling_sp_Block1_Zone2 = 0.5,
    set pmv_heating_sp_Block1_Zone2 = -0.5,
    set tolerance_cooling_sp_cooling_season_Block1_Zone2 = -0.1,
    set tolerance_cooling_sp_heating_season_Block1_Zone2 = -0.1,
    set tolerance_heating_sp_cooling_season_Block1_Zone2 = 0.25,
    set tolerance_heating_sp_heating_season_Block1_Zone2 = 0.25;

ENERGYMANAGEMENTSYSTEM:PROGRAM,
    apply_aPMV_Block1_Zone2,    !- Name
    if CoolingSeason == 1,
        set adap_coeff_Block1_Zone2 = adap_coeff_cooling_Block1_Zone2,
        set tolerance_cooling_sp_Block1_Zone2 = tolerance_cooling_sp_cooling_season_Block1_Zone2,
        set tolerance_heating_sp_Block1_Zone2 = tolerance_heating_sp_cooling_season_Block1_Zone2,
    elseif CoolingSeason == 0,
        set adap_coeff_Block1_Zone2 = adap_coeff_heating_Block1_Zone2,
        set tolerance_cooling_sp_Block1_Zone2 = tolerance_cooling_sp_heating_season_Block1_Zone2,
        set tolerance_heating_sp_Block1_Zone2 = tolerance_heating_sp_heating_season_Block1_Zone2,
    endif,
    set aPMV_H_SP_noTol_Block1_Zone2 = pmv_heating_sp_Block1_Zone2/(1+adap_coeff_Block1_Zone2*pmv_heating_sp_Block1_Zone2),
    set aPMV_C_SP_noTol_Block1_Zone2 = pmv_cooling_sp_Block1_Zone2/(1+adap_coeff_Block1_Zone2*pmv_cooling_sp_Block1_Zone2),
    set aPMV_H_SP_Block1_Zone2 = aPMV_H_SP_noTol_Block1_Zone2+tolerance_heating_sp_Block1_Zone2,
    set aPMV_C_SP_Block1_Zone2 = aPMV_C_SP_noTol_Block1_Zone2+tolerance_cooling_sp_Block1_Zone2,
    if People_Occupant_Count_Block1_Zone2 > 0,
        if aPMV_H_SP_Block1_Zone2 < 0,
            set PMV_H_SP_act_Block1_Zone2 = aPMV_H_SP_Block1_Zone2,
        else,
            set PMV_H_SP_act_Block1_Zone2 = 0,
        endif,
        if aPMV_C_SP_Block1_Zone2 > 0,
            set PMV_C_SP_act_Block1_Zone2 = aPMV_C_SP_Block1_Zone2,
        else,
            set PMV_C_SP_act_Block1_Zone2 = 0,
        endif,
    else,
        set PMV_H_SP_act_Block1_Zone2 = -100,
        set PMV_C_SP_act_Block1_Zone2 = 100,
    endif;

ENERGYMANAGEMENTSYSTEM:PROGRAM,
    monitor_aPMV_Block1_Zone2,    !- Name
    set aPMV_Block1_Zone2 = PMV_Block1_Zone2/(1+adap_coeff_Block1_Zone2*PMV_Block1_Zone2);

ENERGYMANAGEMENTSYSTEM:PROGRAM,
    count_aPMV_comfort_hours_Block1_Zone2,    !- Name
    if aPMV_Block1_Zone2 < aPMV_H_SP_noTol_Block1_Zone2,
        set comfhour_Block1_Zone2 = 0,
        set discomfhour_cold_Block1_Zone2 = 1*ZoneTimeStep,
        set discomfhour_heat_Block1_Zone2 = 0,
    elseif aPMV_Block1_Zone2 > aPMV_C_SP_noTol_Block1_Zone2,
        set comfhour_Block1_Zone2 = 0,
        set discomfhour_cold_Block1_Zone2 = 0,
        set discomfhour_heat_Block1_Zone2 = 1*ZoneTimeStep,
    else, 
        set comfhour_Block1_Zone2 = 1*ZoneTimeStep,
        set discomfhour_cold_Block1_Zone2 = 0,
        set discomfhour_heat_Block1_Zone2 = 0,
    endif,                
    if People_Occupant_Count_Block1_Zone2 > 0,
        set occupied_hour_Block1_Zone2 = 1*ZoneTimeStep,
    else,                 
        set occupied_hour_Block1_Zone2 = 0,
    endif,                
    set discomfhour_Block1_Zone2 = discomfhour_cold_Block1_Zone2 + discomfhour_heat_Block1_Zone2;

