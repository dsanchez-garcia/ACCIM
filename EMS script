    Averaged,                 !- Type of Data in Variable
    ZoneTimestep,             !- Update Frequency
    ,                         !- EMS Program or Subroutine Name
    ;                         !- Units

ENERGYMANAGEMENTSYSTEM:OUTPUTVARIABLE,
    aPMV Cooling Setpoint No Tolerance_Block1_Zone1,    !- Name
    aPMV_C_SP_noTol_Block1_Zone1,    !- EMS Variable Name
    Averaged,                 !- Type of Data in Variable
    ZoneTimestep,             !- Update Frequency
    ,                         !- EMS Program or Subroutine Name
    ;                         !- Units

ENERGYMANAGEMENTSYSTEM:OUTPUTVARIABLE,
    Comfortable Hours_Block1_Zone1,    !- Name
    comfhour_Block1_Zone1,    !- EMS Variable Name
    Summed,                   !- Type of Data in Variable
    ZoneTimestep,             !- Update Frequency
    ,                         !- EMS Program or Subroutine Name
    H;                        !- Units

ENERGYMANAGEMENTSYSTEM:OUTPUTVARIABLE,
    Discomfortable Hot Hours_Block1_Zone1,    !- Name
    discomfhour_heat_Block1_Zone1,    !- EMS Variable Name
    Summed,                   !- Type of Data in Variable
    ZoneTimestep,             !- Update Frequency
