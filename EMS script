ENERGYMANAGEMENTSYSTEM:GLOBALVARIABLE,
    aPMV_C_SP_noTol_Block1_Zone2;    !- Erl Variable 1 Name

! 1.3. EMS objects for all zones

ENERGYMANAGEMENTSYSTEM:GLOBALVARIABLE,
    CoolingSeason;            !- Erl Variable 1 Name

ENERGYMANAGEMENTSYSTEM:GLOBALVARIABLE,
    CoolSeasonEnd;            !- Erl Variable 1 Name

ENERGYMANAGEMENTSYSTEM:GLOBALVARIABLE,
    CoolSeasonStart;          !- Erl Variable 1 Name

! 2. Stage: Programs

! 2.1. ProgramCallingManager objects

! 2.1.1. ProgramCallingManager objects for all zones

ENERGYMANAGEMENTSYSTEM:PROGRAMCALLINGMANAGER,
    set_cooling_season_input_data,    !- Name
    BeginTimestepBeforePredictor,    !- EnergyPlus Model Calling Point
    set_cooling_season_input_data;    !- Program Name 1

ENERGYMANAGEMENTSYSTEM:PROGRAMCALLINGMANAGER,
    set_cooling_season,       !- Name
    BeginTimestepBeforePredictor,    !- EnergyPlus Model Calling Point
    set_cooling_season;       !- Program Name 1

 ! 2.1.2. ProgramCallingManager objects for Block1:Zone1

 ENERGYMANAGEMENTSYSTEM:PROGRAMCALLINGMANAGER,
    set_zone_input_data_Block1_Zone1,    !- Name
    BeginTimestepBeforePredictor,    !- EnergyPlus Model Calling Point
    set_zone_input_data_Block1_Zone1;    !- Program Name 1

ENERGYMANAGEMENTSYSTEM:PROGRAMCALLINGMANAGER,
    apply_aPMV_Block1_Zone1,    !- Name
    BeginTimestepBeforePredictor,    !- EnergyPlus Model Calling Point
    apply_aPMV_Block1_Zone1;    !- Program Name 1

ENERGYMANAGEMENTSYSTEM:PROGRAMCALLINGMANAGER,
    monitor_aPMV_Block1_Zone1,    !- Name
    BeginTimestepBeforePredictor,    !- EnergyPlus Model Calling Point
    monitor_aPMV_Block1_Zone1;    !- Program Name 1

ENERGYMANAGEMENTSYSTEM:PROGRAMCALLINGMANAGER,
    count_aPMV_comfort_hours_Block1_Zone1,    !- Name
    BeginTimestepBeforePredictor,    !- EnergyPlus Model Calling Point
    count_aPMV_comfort_hours_Block1_Zone1;    !- Program Name 1

 ! 2.1.3. ProgramCallingManager objects for Block1:Zone2

ENERGYMANAGEMENTSYSTEM:PROGRAMCALLINGMANAGER,
    set_zone_input_data_Block1_Zone2,    !- Name
    BeginTimestepBeforePredictor,    !- EnergyPlus Model Calling Point
    set_zone_input_data_Block1_Zone2;    !- Program Name 1

ENERGYMANAGEMENTSYSTEM:PROGRAMCALLINGMANAGER,
    apply_aPMV_Block1_Zone2,    !- Name
    BeginTimestepBeforePredictor,    !- EnergyPlus Model Calling Point
    apply_aPMV_Block1_Zone2;    !- Program Name 1

ENERGYMANAGEMENTSYSTEM:PROGRAMCALLINGMANAGER,
    monitor_aPMV_Block1_Zone2,    !- Name
    BeginTimestepBeforePredictor,    !- EnergyPlus Model Calling Point
    monitor_aPMV_Block1_Zone2;    !- Program Name 1

ENERGYMANAGEMENTSYSTEM:PROGRAMCALLINGMANAGER,
    count_aPMV_comfort_hours_Block1_Zone2,    !- Name
    BeginTimestepBeforePredictor,    !- EnergyPlus Model Calling Point
    count_aPMV_comfort_hours_Block1_Zone2;    !- Program Name 1

! 2.2. Program objects

! 2.2.1. Program objects for all zones

ENERGYMANAGEMENTSYSTEM:PROGRAM,
    set_cooling_season_input_data,    !- Name
    set CoolSeasonStart = 91,    !- Program Line 1
    set CoolSeasonEnd = 274;    !- Program Line 2

ENERGYMANAGEMENTSYSTEM:PROGRAM,
    set_cooling_season,       !- Name
    if CoolSeasonEnd > CoolSeasonStart,    !- Program Line 1
        if (DayOfYear >= CoolSeasonStart) && (DayOfYear < CoolSeasonEnd),    !- Program Line 2
            set CoolingSeason = 1,    !- Program Line 3
        else,                     !- Program Line 4
            set CoolingSeason = 0,    !- Program Line 5
        endif,                    !- Program Line 6
    elseif CoolSeasonStart > CoolSeasonEnd,    !- Program Line 7
        if (DayOfYear >= CoolSeasonStart) || (DayOfYear < CoolSeasonEnd),    !- Program Line 8
            set CoolingSeason = 1,    !- Program Line 9
        else,                     !- Program Line 10
            set CoolingSeason = 0,    !- Program Line 11
        endif,                    !- Program Line 12
    endif;                    !- Program Line 13

! 2.2.2. Program objects for Block1:Zone1

ENERGYMANAGEMENTSYSTEM:PROGRAM,
    set_zone_input_data_Block1_Zone1,    !- Name
    set adap_coeff_cooling_Block1_Zone1 = 0.3,    !- Program Line 1
    set adap_coeff_heating_Block1_Zone1 = -0.3,    !- Program Line 2
    set pmv_cooling_sp_Block1_Zone1 = 0.5,    !- Program Line 3
    set pmv_heating_sp_Block1_Zone1 = -0.5,    !- Program Line 4
    set tolerance_cooling_sp_cooling_season_Block1_Zone1 = -0.1,    !- Program Line 5
    set tolerance_cooling_sp_heating_season_Block1_Zone1 = -0.1,    !- Program Line 6
    set tolerance_heating_sp_cooling_season_Block1_Zone1 = 0.25,    !- Program Line 7
    set tolerance_heating_sp_heating_season_Block1_Zone1 = 0.25;    !- Program Line 8

ENERGYMANAGEMENTSYSTEM:PROGRAM,
    apply_aPMV_Block1_Zone1,    !- Name
    if CoolingSeason == 1,    !- Program Line 1
        set adap_coeff_Block1_Zone1 = adap_coeff_cooling_Block1_Zone1,    !- Program Line 2
        set tolerance_cooling_sp_Block1_Zone1 = tolerance_cooling_sp_cooling_season_Block1_Zone1,    !- Program Line 3
        set tolerance_heating_sp_Block1_Zone1 = tolerance_heating_sp_cooling_season_Block1_Zone1,    !- Program Line 4
    elseif CoolingSeason == 0,    !- Program Line 5
        set adap_coeff_Block1_Zone1 = adap_coeff_heating_Block1_Zone1,    !- Program Line 6
        set tolerance_cooling_sp_Block1_Zone1 = tolerance_cooling_sp_heating_season_Block1_Zone1,    !- Program Line 7
        set tolerance_heating_sp_Block1_Zone1 = tolerance_heating_sp_heating_season_Block1_Zone1,    !- Program Line 8
    endif,                    !- Program Line 9
    set aPMV_H_SP_noTol_Block1_Zone1 = pmv_heating_sp_Block1_Zone1/(1+adap_coeff_Block1_Zone1*pmv_heating_sp_Block1_Zone1),    !- Program Line 10
    set aPMV_C_SP_noTol_Block1_Zone1 = pmv_cooling_sp_Block1_Zone1/(1+adap_coeff_Block1_Zone1*pmv_cooling_sp_Block1_Zone1),    !- Program Line 11
    set aPMV_H_SP_Block1_Zone1 = aPMV_H_SP_noTol_Block1_Zone1+tolerance_heating_sp_Block1_Zone1,    !- Program Line 12
    set aPMV_C_SP_Block1_Zone1 = aPMV_C_SP_noTol_Block1_Zone1+tolerance_cooling_sp_Block1_Zone1,    !- Program Line 13
    if People_Occupant_Count_Block1_Zone1 > 0,    !- Program Line 14
        if aPMV_H_SP_Block1_Zone1 < 0,    !- Program Line 15
            set PMV_H_SP_act_Block1_Zone1 = aPMV_H_SP_Block1_Zone1,    !- Program Line 16
        else,                     !- Program Line 17
            set PMV_H_SP_act_Block1_Zone1 = 0,    !- Program Line 18
        endif,                    !- Program Line 19
        if aPMV_C_SP_Block1_Zone1 > 0,    !- Program Line 20
            set PMV_C_SP_act_Block1_Zone1 = aPMV_C_SP_Block1_Zone1,    !- Program Line 21
        else,                     !- Program Line 22
            set PMV_C_SP_act_Block1_Zone1 = 0,    !- Program Line 23
        endif,                    !- Program Line 24
    else,                     !- Program Line 25
        set PMV_H_SP_act_Block1_Zone1 = -100,    !- Program Line 26
        set PMV_C_SP_act_Block1_Zone1 = 100,    !- Program Line 27
    endif;                    !- Program Line 28

ENERGYMANAGEMENTSYSTEM:PROGRAM,
    monitor_aPMV_Block1_Zone1,    !- Name
    set aPMV_Block1_Zone1 = PMV_Block1_Zone1/(1+adap_coeff_Block1_Zone1*PMV_Block1_Zone1);    !- Program Line 1

ENERGYMANAGEMENTSYSTEM:PROGRAM,
    count_aPMV_comfort_hours_Block1_Zone1,    !- Name
    if aPMV_Block1_Zone1 < aPMV_H_SP_noTol_Block1_Zone1,    !- Program Line 1
        set comfhour_Block1_Zone1 = 0,    !- Program Line 2
        set discomfhour_cold_Block1_Zone1 = 1*ZoneTimeStep,    !- Program Line 3
        set discomfhour_heat_Block1_Zone1 = 0,    !- Program Line 4
    elseif aPMV_Block1_Zone1 > aPMV_C_SP_noTol_Block1_Zone1,    !- Program Line 5
        set comfhour_Block1_Zone1 = 0,    !- Program Line 6
        set discomfhour_cold_Block1_Zone1 = 0,    !- Program Line 7
        set discomfhour_heat_Block1_Zone1 = 1*ZoneTimeStep,    !- Program Line 8
    else,                     !- Program Line 9
        set comfhour_Block1_Zone1 = 1*ZoneTimeStep,    !- Program Line 10
        set discomfhour_cold_Block1_Zone1 = 0,    !- Program Line 11
        set discomfhour_heat_Block1_Zone1 = 0,    !- Program Line 12
    endif,                    !- Program Line 13
    if People_Occupant_Count_Block1_Zone1 > 0,    !- Program Line 14
        set occupied_hour_Block1_Zone1 = 1*ZoneTimeStep,    !- Program Line 15
    else,                     !- Program Line 16
        set occupied_hour_Block1_Zone1 = 0,    !- Program Line 17
    endif,                    !- Program Line 18
    set discomfhour_Block1_Zone1 = discomfhour_cold_Block1_Zone1 + discomfhour_heat_Block1_Zone1;    !- Program Line 19

! 2.2.3. Program objects for Block1:Zone2

ENERGYMANAGEMENTSYSTEM:PROGRAM,
    set_zone_input_data_Block1_Zone2,    !- Name
    set adap_coeff_cooling_Block1_Zone2 = 0.25,    !- Program Line 1
    set adap_coeff_heating_Block1_Zone2 = -0.25,    !- Program Line 2
    set pmv_cooling_sp_Block1_Zone2 = 0.5,    !- Program Line 3
    set pmv_heating_sp_Block1_Zone2 = -0.5,    !- Program Line 4
    set tolerance_cooling_sp_cooling_season_Block1_Zone2 = -0.1,    !- Program Line 5
    set tolerance_cooling_sp_heating_season_Block1_Zone2 = -0.1,    !- Program Line 6
    set tolerance_heating_sp_cooling_season_Block1_Zone2 = 0.25,    !- Program Line 7
    set tolerance_heating_sp_heating_season_Block1_Zone2 = 0.25;    !- Program Line 8

ENERGYMANAGEMENTSYSTEM:PROGRAM,
    apply_aPMV_Block1_Zone2,    !- Name
    if CoolingSeason == 1,    !- Program Line 1
        set adap_coeff_Block1_Zone2 = adap_coeff_cooling_Block1_Zone2,    !- Program Line 2
        set tolerance_cooling_sp_Block1_Zone2 = tolerance_cooling_sp_cooling_season_Block1_Zone2,    !- Program Line 3
        set tolerance_heating_sp_Block1_Zone2 = tolerance_heating_sp_cooling_season_Block1_Zone2,    !- Program Line 4
    elseif CoolingSeason == 0,    !- Program Line 5
        set adap_coeff_Block1_Zone2 = adap_coeff_heating_Block1_Zone2,    !- Program Line 6
        set tolerance_cooling_sp_Block1_Zone2 = tolerance_cooling_sp_heating_season_Block1_Zone2,    !- Program Line 7
        set tolerance_heating_sp_Block1_Zone2 = tolerance_heating_sp_heating_season_Block1_Zone2,    !- Program Line 8
    endif,                    !- Program Line 9
    set aPMV_H_SP_noTol_Block1_Zone2 = pmv_heating_sp_Block1_Zone2/(1+adap_coeff_Block1_Zone2*pmv_heating_sp_Block1_Zone2),    !- Program Line 10
    set aPMV_C_SP_noTol_Block1_Zone2 = pmv_cooling_sp_Block1_Zone2/(1+adap_coeff_Block1_Zone2*pmv_cooling_sp_Block1_Zone2),    !- Program Line 11
    set aPMV_H_SP_Block1_Zone2 = aPMV_H_SP_noTol_Block1_Zone2+tolerance_heating_sp_Block1_Zone2,    !- Program Line 12
    set aPMV_C_SP_Block1_Zone2 = aPMV_C_SP_noTol_Block1_Zone2+tolerance_cooling_sp_Block1_Zone2,    !- Program Line 13
    if People_Occupant_Count_Block1_Zone2 > 0,    !- Program Line 14
        if aPMV_H_SP_Block1_Zone2 < 0,    !- Program Line 15
            set PMV_H_SP_act_Block1_Zone2 = aPMV_H_SP_Block1_Zone2,    !- Program Line 16
        else,                     !- Program Line 17
            set PMV_H_SP_act_Block1_Zone2 = 0,    !- Program Line 18
        endif,                    !- Program Line 19
        if aPMV_C_SP_Block1_Zone2 > 0,    !- Program Line 20
            set PMV_C_SP_act_Block1_Zone2 = aPMV_C_SP_Block1_Zone2,    !- Program Line 21
        else,                     !- Program Line 22
            set PMV_C_SP_act_Block1_Zone2 = 0,    !- Program Line 23
        endif,                    !- Program Line 24
    else,                     !- Program Line 25
        set PMV_H_SP_act_Block1_Zone2 = -100,    !- Program Line 26
        set PMV_C_SP_act_Block1_Zone2 = 100,    !- Program Line 27
    endif;                    !- Program Line 28

ENERGYMANAGEMENTSYSTEM:PROGRAM,
    monitor_aPMV_Block1_Zone2,    !- Name
    set aPMV_Block1_Zone2 = PMV_Block1_Zone2/(1+adap_coeff_Block1_Zone2*PMV_Block1_Zone2);    !- Program Line 1

ENERGYMANAGEMENTSYSTEM:PROGRAM,
    count_aPMV_comfort_hours_Block1_Zone2,    !- Name
    if aPMV_Block1_Zone2 < aPMV_H_SP_noTol_Block1_Zone2,    !- Program Line 1
        set comfhour_Block1_Zone2 = 0,    !- Program Line 2
        set discomfhour_cold_Block1_Zone2 = 1*ZoneTimeStep,    !- Program Line 3
        set discomfhour_heat_Block1_Zone2 = 0,    !- Program Line 4
    elseif aPMV_Block1_Zone2 > aPMV_C_SP_noTol_Block1_Zone2,    !- Program Line 5
        set comfhour_Block1_Zone2 = 0,    !- Program Line 6
        set discomfhour_cold_Block1_Zone2 = 0,    !- Program Line 7
        set discomfhour_heat_Block1_Zone2 = 1*ZoneTimeStep,    !- Program Line 8
    else,                     !- Program Line 9
        set comfhour_Block1_Zone2 = 1*ZoneTimeStep,    !- Program Line 10
        set discomfhour_cold_Block1_Zone2 = 0,    !- Program Line 11
        set discomfhour_heat_Block1_Zone2 = 0,    !- Program Line 12
    endif,                    !- Program Line 13
    if People_Occupant_Count_Block1_Zone2 > 0,    !- Program Line 14
        set occupied_hour_Block1_Zone2 = 1*ZoneTimeStep,    !- Program Line 15
    else,                     !- Program Line 16
        set occupied_hour_Block1_Zone2 = 0,    !- Program Line 17
    endif,                    !- Program Line 18
    set discomfhour_Block1_Zone2 = discomfhour_cold_Block1_Zone2 + discomfhour_heat_Block1_Zone2;    !- Program Line 19

! 3. Stage: Output variables

